<h1>Items</h1>
<% @items.each do |item| %>
  <h3> <%= link_to "#{item.name}", "/items/#{item.id}" %> </h3>
  <p>
    <%= item.image%>
    <%= item.price%>
    <%= item.description%>
    <%= item.active%>
    <%= item.in_stock%>
    <%= item.inventory%>
    <%= Merchant.find(item.merchant_id).name%>
  </p>
  <hr>
<% end %>
